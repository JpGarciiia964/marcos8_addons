<?xml version="1.0" encoding="utf-8"?>
<openerp>

    <data>
        <template id="assets_backend" name="custom bar assets" inherit_id="web.assets_backend">
            <xpath expr="." position="inside">
                <link rel="stylesheet" href="/visanet_colors/static/src/css/main.css"/>
            </xpath>
        </template>
    </data>

    <template id="MYSITE_layout" inherit_id="website.layout" name="MYSITE layout" priority="17">
        <xpath expr="//head//link" position="after">
            <link rel="apple-touch-icon" sizes="57x57" href="/visanet_colors/static/src/img/apple-icon-57x57.png"/>
            <link rel="apple-touch-icon" sizes="60x60" href="/visanet_colors/static/src/img/apple-icon-60x60.png"/>
            <link rel="apple-touch-icon" sizes="72x72" href="/visanet_colors/static/src/img/apple-icon-72x72.png"/>
            <link rel="apple-touch-icon" sizes="76x76" href="/visanet_colors/static/src/img/apple-icon-76x76.png"/>
            <link rel="apple-touch-icon" sizes="114x114" href="/visanet_colors/static/src/img/apple-icon-114x114.png"/>
            <link rel="apple-touch-icon" sizes="120x120" href="/visanet_colors/static/src/img/apple-icon-120x120.png"/>
            <link rel="apple-touch-icon" sizes="144x144" href="/visanet_colors/static/src/img/apple-icon-144x144.png"/>
            <link rel="apple-touch-icon" sizes="152x152" href="/visanet_colors/static/src/img/apple-icon-152x152.png"/>
            <link rel="apple-touch-icon" sizes="180x180" href="/visanet_colors/static/src/img/apple-icon-180x180.png"/>
            <link rel="icon" type="image/png" sizes="192x192"  href="/visanet_colors/static/src/img/android-icon-192x192.png"/>
            <link rel="icon" type="image/png" sizes="32x32" href="/visanet_colors/static/src/img/favicon-32x32.png"/>
            <link rel="icon" type="image/png" sizes="96x96" href="/visanet_colors/static/src/img/favicon-96x96.png"/>
            <link rel="icon" type="image/png" sizes="16x16" href="/visanet_colors/static/src/img/favicon-16x16.png"/>
            <link rel="manifest" href="/manifest.json"/>
            <meta name="msapplication-TileColor" content="#ffffff"/>
            <meta name="msapplication-TileImage" content="/visanet_colors/static/src/img/ms-icon-144x144.png"/>
            <meta name="theme-color" content="#ffffff"/>
        </xpath>
    </template>

</openerp>

